<h1>Добавени песни</h1>

{{#each findedMusic}}
<div class="card" style="width: 18rem;">
    <iframe width="420" height="315" src="https://www.youtube.com/embed/{{this.URL}}" class="card-img-top"></iframe>
    <div class="card-body">
        <h5 class="card-title">{{this.title}}</h5>
        <p class="card-text">{{this.singer}}</p>
        <p class="card-text">{{this.ganre}}</p>

    </div>
</div>
{{/each}}